						<property name="salary" value="40000.0"></property>
					</bean>
					<bean id="101" class="com.cg.bean.Employee">
						<property name="employeeId" value="101"></property>
						<property name="employeeName" value="Harriet"></property>
						<property name="salary" value="40000.0"></property>
					</bean>
			</list>
		</property>
	</bean>

</beans>
